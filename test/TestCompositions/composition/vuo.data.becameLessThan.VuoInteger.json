{"portConfiguration":{
	"equal":                {"firingPort":"a", "inputPortValues":{"a": 0,"b": 0}},
	"became greater":       {"firingPort":"a", "inputPortValues":{"a": 1,"b": 0}},
	"became less":          {"firingPort":"a", "inputPortValues":{"a": 1,"b": 2}, "outputPortValues":{"becameLessThan":null}},
	"still less":           {"firingPort":"a", "inputPortValues":{"a": 0,"b": 2}},
	"became greater again": {"firingPort":"a", "inputPortValues":{"a": 0,"b":-1}},
	"became less again":    {"firingPort":"a", "inputPortValues":{"a":-2,"b":-1}, "outputPortValues":{"becameLessThan":null}},
}}

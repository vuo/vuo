	"1 point" :{"firingPort":"center", "inputPortValues":{"numberOfPoints": 1}, "outputPortValues":{"ovalPoints":[{"x":0.5,"y":0}                                 ]}},
	"2 points":{"firingPort":"center", "inputPortValues":{"numberOfPoints": 2}, "outputPortValues":{"ovalPoints":[{"x":0.5,"y":0},{"x":-0.5,"y":0}                ]}},
	"3 points":{"firingPort":"center", "inputPortValues":{"numberOfPoints": 3}, "outputPortValues":{"ovalPoints":[{"x":0.5,"y":0},{"x":0,"y":0.5},{"x":-0.5,"y":0}]}},
}}
